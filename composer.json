{
  "name": "ddme/craft",
  "description": "DDME Craft CMS Scaffolding",
  "version": "1.1.1",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "craftcms/cms": "^3.0.1",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/mailgun": "^1.2.0",
    "craftcms/redactor": "^1.0.1",
    "craftcms/aws-s3": "^1.0.8",
    "craftcms/contact-form": "^2.1.1",
    "verbb/super-table": "^2.0.4",
    "barrelstrength/sprout-fields": "^3.0.3",
    "ether/simplemap": "^3.3.3",
    "verbb/expanded-singles": "^1.0.3",
    "dolphiq/sitemap": "^v1.0.9",
    "topshelfcraft/environment-label": "^3.1.2",
    "craftcms/contact-form-honeypot": "^1.0.2",
    "nystudio107/craft-minify": "^1.2.8",
    "nystudio107/craft-typogrify": "^1.1.9",
    "nystudio107/craft-scripts": "^1.2.4"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup/welcome",
      "@php marlo welcome"
    ],
    "post-update-cmd": [
      "@php marlo update"
    ],
    "post-install-cmd": [
      "@php marlo update"
    ]
  }
}
